<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="处理一个异步流程\n 网络请求、数据库操作、文件读写，UI展示" />

    <Button
        android:id="@+id/btn2_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="延迟执行,（发射一个数据 0）" />

    <Button
        android:id="@+id/btn3_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="两个被观察者合并数据  执行同一个观察者" />

    <Button
        android:id="@+id/btn4_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="定时任务" />

    <Button
        android:id="@+id/btn5_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="合并最近的N个节点\n ( 比如说 注册：需要三个参数数同时满足)" />

    <Button
        android:id="@+id/btn6_activityMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="合并最近的N个节点\n ( 比如说 注册：需要三个参数数同时满足)" />

</LinearLayout>